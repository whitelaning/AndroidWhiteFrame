<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whitelaning.whiteframe" >

    <application
        android:name=".application.WhiteFrame"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.StartActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.whitelaning.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.whitelaning.activity.WhiteAdapterDemoActivity"
            android:label="@string/title_activity_white_adapter_demo" >
        </activity>
        <activity
            android:name="com.whitelaning.activity.ListViewActivity"
            android:label="@string/title_activity_list_view" >
        </activity>
        <activity
            android:name="com.whitelaning.activity.TestActivity"
            android:label="@string/title_activity_test" >
        </activity>
        <activity
            android:name="com.whitelaning.activity.UrlQuerySanitizerActivity"
            android:label="@string/title_activity_url_query_sanitizer" >
        </activity>
    </application>

</manifest>
